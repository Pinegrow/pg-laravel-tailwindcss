{
  "name": "pg-laravel-tailwindcss",
  "version": "0.0.0",
  "homepage": "https://pg-laravel-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-laravel-tailwindcss.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "now": "npm run build && npm run preview",
    "lighthouse:dev": "npx unlighthouse --no-cache --site http://127.0.0.1:8000",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-laravel-tailwindcss.netlify.app",
    "build": "vite build && vite build --ssr",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write . !public"
  },
  "dependencies": {
    "@vueuse/core": "^10.0.2",
    "defu": "^6.1.2",
    "pinia": "^2.1.6",
    "vee-validate": "^4.11.6"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.149",
    "@inertiajs/vue3": "^1.0.0",
    "@nuxt/eslint-config": "^0.2.0",
    "@pinegrow/tailwindcss-plugin": "^3.0.18",
    "@pinegrow/vite-plugin": "^3.0.34",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^20.10.0",
    "@unocss/preset-icons": "^0.57.7",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/devtools": "^6.5.1",
    "@vue/server-renderer": "^3.2.31",
    "autoprefixer": "^10.4.7",
    "axios": "^1.6.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "laravel-vite-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "ofetch": "^1.0.1",
    "postcss": "^8.4.14",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.3.2",
    "unocss": "^0.57.7",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.0.0",
    "vite-plugin-vue-devtools": "^1.0.0-rc.5",
    "vue": "^3.2.31",
    "vue-tsc": "^1.8.24"
  },
  "web-types": "web-types.json"
}
