{
  "name": "pg-laravel-tailwindcss",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://pg-laravel-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-laravel-tailwindcss.git"
  },
  "scripts": {
    "dev": "vite",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-laravel-tailwindcss.netlify.app",
    "build": "vite build && vite build --ssr",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix ./",
    "format": "prettier --write \"**/*.{html,vue,svelte,astro,js,jsx,cjs,mjs,ts,tsx,cts,mts,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^12.4.0",
    "defu": "^6.1.4",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.0",
    "vee-validate": "^4.15.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.295",
    "@inertiajs/vue3": "^2.0.0",
    "@nuxt/eslint-config": "^0.7.5",
    "@pinegrow/tailwindcss-plugin": "^3.0.35",
    "@pinegrow/vite-plugin": "^3.0.69",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.6",
    "@unocss/preset-icons": "^65.4.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/server-renderer": "^3.5.13",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "laravel-vite-plugin": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.0",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "unocss": "^65.4.0",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.7",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "web-types": "web-types.json"
}
